Start
Set scores = [2.9, 2.6, 3.2, 2.65, 2.75]
Set Suitable_count = 0
Set reject_count = 0
Set tot_students = 0
Set CGPA = 2.75

For each score in scores
	tot_students = tot_students + 1
	If score >= CGPA
		Then Suitable_count = Suitable_count +1
	Else
		reject_count = reject_count + 1
	End if
End for
Print tot_students
Print Suitable_count
Print reject_count
End
